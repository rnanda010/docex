TARGET=rc/justcheck
MASTER=origin/master
HOTFIX=origin/
hotfix/HyperCare_XD


MB_MASTER=$(git merge-base $TARGET $MASTER)
MB_HOTFIX=$(git merge-base $TARGET $HOTFIX)

DIST_MASTER=$(git rev-list --count $MB_MASTER..$MASTER)
DIST_HOTFIX=$(git rev-list --count $MB_HOTFIX..$HOTFIX)

if [ $DIST_MASTER -lt $DIST_HOTFIX ]; then
  echo "Branch was created from: master"
else
  echo "Branch was created from: hotfix/Hypercare_XD"
fi
